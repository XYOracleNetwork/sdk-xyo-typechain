{
  "name": "@xyo-network/typechain",
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@xyo-network/solidity": "workspace:^"
  },
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain.git"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "devDependencies": {
    "@typechain/ethers-v5": "^10.2.1",
    "@xylabs/ts-scripts-yarn3": "^3.1.2",
    "ethers": "^5.7.2",
    "rimraf": "^5.0.5",
    "typechain": "^8.3.2",
    "typescript": "^5.2.2"
  },
  "docs": "dist/docs.json",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.cjs"
      },
      "browser": {
        "import": "./dist/browser/index.js",
        "require": "./dist/browser/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.js",
  "types": "dist/node/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "license": "LGPL-3.0",
  "scripts": {
    "package-clean": "rimraf dist && rimraf src",
    "package-compile": "yarn generate:sdk && yarn compile:sdk",
    "package-recompile": "yarn package-clean && yarn package-compile",
    "generate:sdk": "typechain --out-dir=./src --target=ethers-v5 ../../node_modules/@xyo-network/solidity/build/contracts/**/*.json",
    "compile:sdk": "yarn package-compile-only",
    "lint-pkg": "npmPkgJsonLint ."
  },
  "sideEffects": false,
  "version": "2.2.9",
  "type": "module"
}

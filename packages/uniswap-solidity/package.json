    "tslib": "^2.4.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf src && rm -r -f build && yarn compile",
    "clean": "rm -r -f ./build && rm -r -f ./dist && rm -r -f ./src && rm -r -f ./node_modules && rm -r -f ./lib",
    "compile": "rimraf build && yarn copy-contracts && truffle compile --config truffle-config.cjs",
    "copy-contracts": "copyfiles -u 6 \"../../node_modules/@uniswap/v3-core/contracts/**/*\" contracts",
    "lint-pkg": "npmPkgJsonLint ."
  },
  "sideEffects": false,
  "version": "2.0.4"
}

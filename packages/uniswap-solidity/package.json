    "abstract-leveldown": "^7.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.32.0",
    "google-protobuf": "^3.21.2",
    "graphql": "^16.6.0",
    "jest": "^29.3.1",
    "jest-sorted": "^1.0.14",
    "node-fetch": "^3.3.0",
    "npm-package-json-lint": "^6.4.0",
    "npm-package-json-lint-config-default": "^5.0.0",

{
  "name": "@xyo-network/solidity",
  "version": "4.0.0",
  "bugs": {
    "url": "git+https://github.com/XYOracleNetwork/sdk-xyo-typechain/issues",
    "email": "support@xyo.network"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XYOracleNetwork/sdk-xyo-typechain.git"
  },
  "license": "LGPL-3.0-only",
  "author": {
    "name": "XYO Development Team",
    "email": "support@xyo.network",
    "url": "https://xyo.network"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./typechain-types/index.ts"
    },
    "./package.json": "./package.json",
    "./README.md": "./README.md"
  },
  "files": [
    "contracts/",
    "artifacts/contracts/",
    "typechain-types/"
  ],
  "scripts": {
    "dev": "hardhat node",
    "dev-deploy": "NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only --no-warnings=ExperimentalWarning' hardhat ignition deploy ignition/modules/BridgeableToken.ts --network localhost",
    "package-build": "yarn package-clean && yarn package-compile",
    "package-clean": "hardhat clean",
    "package-compile": "hardhat compile",
    "package-coverage": "NODE_OPTIONS='--experimental-loader ts-node/esm/transpile-only --no-warnings=ExperimentalWarning' SOLIDITY_COVERAGE=true TS_NODE_TRANSPILE_ONLY=true hardhat coverage --solcoverjs ./.solcover.json",
    "package-lint": "echo Skipping lint...",
    "package-recompile": "yarn package-clean && yarn package-compile",
    "package-test": "hardhat test"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ethers": "~4.0.0",
    "@nomicfoundation/hardhat-ethers-chai-matchers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^3.0.1",
    "@nomicfoundation/hardhat-ignition-ethers": "~3.0.1",
    "@nomicfoundation/hardhat-keystore": "^3.0.1",
    "@nomicfoundation/hardhat-mocha": "^3.0.1",
    "@nomicfoundation/hardhat-network-helpers": "~3.0.0",
    "@nomicfoundation/hardhat-toolbox-mocha-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-typechain": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "~3.0.0",
    "@nomicfoundation/ignition-core": "^3.0.1",
    "@openzeppelin/contracts": "~5.4.0",
    "@openzeppelin/contracts-upgradeable": "~5.4.0",
    "@typechain/ethers-v6": "~0.5.1",
    "@types/chai": "^5.2.2",
    "@types/chai-as-promised": "^8.0.2",
    "@types/mocha": "^10.0.10",
    "@xylabs/ts-scripts-yarn3": "~7.1.7",
    "@xylabs/tsconfig": "~7.1.7",
    "chai": "~6.0.1",
    "ethers": "~6.15.0",
    "forge-std": "github:foundry-rs/forge-std#v1.9.4",
    "hardhat": "~3.0.2",
    "hardhat-gas-reporter": "~2.3.0",
    "mocha": "^11.0.0",
    "rimraf": "~6.0.1",
    "solidity-coverage": "~0.8.16",
    "truffle": "~5.11.5",
    "ts-node": "~10.9.2",
    "typechain": "~8.3.2",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "ethers": "~6"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "volta": {
    "node": "22.14.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

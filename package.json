{
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "ethers": "^5.6.5"
  },
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain.git"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.6.1",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/providers": "^5.6.5",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/jest": "^27.5.0",
    "@xylabs/eslint-config": "2.3.22",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "abort-controller": "^3.0.0",
    "abstract-leveldown": "^7.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.15.0",
    "google-protobuf": "^3.20.1",
    "graphql": "^16.5.0",
    "jest": "^28.1.0",
    "jest-sorted": "^1.0.14",
    "node-fetch": "^3.2.4",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "subscriptions-transport-ws": "^0.11.0",
    "truffle": "^5.5.13",
    "tslib": "^2.4.0",
    "typechain": "^8.0.0",
    "typescript": "^4.6.4"
  },
  "browser": "dist/esm/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "name": "@xyo-network/sdk-xyo-typechain",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -r -f src && rm -r -f build && yarn compile",
    "clean": "rm -f yarn.lock && rm -f *.tsbuildinfo && rm -r -f ./build && rm -r -f ./dist && rm -r -f ./src && rm -r -f ./node_modules && rm -r -f ./lib",
    "compile": "yarn compile:solidity && yarn compile:typechain && yarn compile:sdk",
    "compile:sdk": "tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json",
    "compile:solidity": "rm -rf ./build/contracts && truffle compile --config truffle-config.cjs",
    "compile:typechain": "rm -rf ./typechain && typechain --out-dir=./src --target=ethers-v5 ./build/contracts/**/*.json",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "deploy": "yarn version --patch && yarn build && yarn publish .",
    "lint-pkg": "npmPkgJsonLint ."
  },
  "sideEffects": false,
  "version": "1.0.2",
  "packageManager": "yarn@3.2.0"
}

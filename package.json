{
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@uniswap/v3-core": "^1.0.1",
    "ethers": "^5.6.8"
  },
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain.git"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.6.3",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/providers": "^5.6.8",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/jest": "^27.5.1",
    "@xylabs/eslint-config": "2.5.79",
    "@xylabs/ts-scripts": "^1.0.66",
    "@xylabs/tsconfig": "^1.0.13",
    "abort-controller": "^3.0.0",
    "abstract-leveldown": "^7.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.16.0",
    "google-protobuf": "^3.20.1",
    "graphql": "^16.5.0",
    "jest": "^28.1.0",
    "jest-sorted": "^1.0.14",
    "node-fetch": "^3.2.4",
    "npm-package-json-lint": "^6.3.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "subscriptions-transport-ws": "^0.11.0",
    "truffle": "^5.5.16",
    "tslib": "^2.4.0",
    "typechain": "^8.0.0",
    "typescript": "^4.7.2"
  },
  "browser": "dist/esm/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "name": "@xyo-network/sdk-xyo-typechain",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf src && rm -r -f build && yarn compile",
    "clean": "rimraf yarn.lock && rimraf *.tsbuildinfo && rm -r -f ./build && rm -r -f ./dist && rm -r -f ./src && rm -r -f ./node_modules && rm -r -f ./lib",
    "compile": "yarn compile:solidity && yarn compile:typechain && yarn compile:sdk",
    "compile:sdk": "rimraf dist && tsc -p tsconfig.build.cjs.json && tsc -p tsconfig.build.esm.json",
    "compile:solidity": "rimraf build && truffle compile --config truffle-config.cjs && truffle compile --config truffle-config-uniswap.cjs",
    "compile:typechain": "rimraf src && typechain --out-dir=./src --target=ethers-v5 ./build/contracts/**/*.json",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "deploy": "yarn version --patch && yarn build && yarn publish .",
    "lint-pkg": "npmPkgJsonLint ."
  },
  "sideEffects": false,
  "version": "1.0.4",
  "packageManager": "yarn@3.2.0"
}

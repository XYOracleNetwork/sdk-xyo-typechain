{
  "name": "@xyo-network/sdk-xyo-typechain",
  "dependencies": {
    "@xyo-network/typechain": "workspace:^",
    "@xyo-network/uniswap-typechain": "workspace:^",
    "@xyo-network/world-typechain": "workspace:^"
  },
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain.git"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@xylabs/eslint-config": "2.12.32",
    "@xylabs/ts-scripts-yarn3": "^2.12.30",
    "@xylabs/tsconfig": "^2.12.30",
    "eslint": "^8.33.0",
    "typescript": "^4.9.5"
  },
  "browser": "dist/esm/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "build": "yarn xy clean && yarn xy build",
    "deploy": "yarn xy clean && yarn workspaces foreach --all version patch --deferred && yarn build && yarn xy gen-docs && yarn version apply --all && workspaces foreach -pt npm publish"
  },
  "version": "2.1.1",
  "packageManager": "yarn@3.4.1",
  "volta": {
    "node": "18.13.0",
    "yarn": "3.4.1"
  }
}

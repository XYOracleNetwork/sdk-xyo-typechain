{
  "name": "@xyo-network/sdk-xyo-typechain",
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.0",
    "@xyo-network/solidity": "workspace:^",
    "@xyo-network/typechain": "workspace:^",
    "@xyo-network/uniswap-solidity": "workspace:^",
    "@xyo-network/uniswap-typechain": "workspace:^",
    "ethers": "^5.7.2"
  },
  "author": {
    "email": "support@xyo.network",
    "name": "XYO Development Team",
    "url": "https://xyo.network"
  },
  "bugs": {
    "email": "support@xyo.network",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-typechain.git"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@typechain/ethers-v5": "^10.1.1",
    "@types/jest": "^29.2.3",
    "@xylabs/eslint-config": "2.7.24",
    "@xylabs/ts-scripts-yarn3": "^2.7.24",
    "@xylabs/tsconfig": "^2.7.24",
    "abort-controller": "^3.0.0",
    "abstract-leveldown": "^7.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.27.0",
    "google-protobuf": "^3.21.2",
    "graphql": "^16.6.0",
    "jest": "^29.3.1",
    "jest-sorted": "^1.0.14",
    "node-fetch": "^3.3.0",
    "npm-package-json-lint": "^6.4.0",
    "npm-package-json-lint-config-default": "^5.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "subscriptions-transport-ws": "^0.11.0",
    "tslib": "^2.4.1",
    "typechain": "^8.1.1",
    "typescript": "^4.8.4"
  },
  "browser": "dist/esm/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn workspaces foreach -pAtv run compile && yarn lint",
    "deploy": "yarn clean && yarn workspaces foreach --all version patch --deferred && yarn build && yarn compile && yarn cycle && yarn version apply --all && yarn workspaces foreach -pt npm publish",
    "clean": "rimraf ./build && rm -r -f ./dist && rimraf ./lib",
    "compile": "yarn tsconfig-gen && yarn package-compile-esm && yarn package-compile-cjs",
    "compile:sdk": "rimraf dist && tsc -p .tsconfig.build.cjs.json && tsc -p .tsconfig.build.esm.json",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "lint-pkg": "npmPkgJsonLint ."
  },
  "sideEffects": false,
  "version": "2.0.4",
  "packageManager": "yarn@3.2.4"
}
